<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.calibre-ebook</string>
    <key>ServiceDescription</key>
    <string>Calibre Content Server</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/username/.local/bin/calibre-server.sh</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/calibre.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/calibre.log</string>
    <key>Debug</key>
    <true/>
</dict>
</plist>
